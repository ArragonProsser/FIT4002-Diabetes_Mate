{
  "version": 1,
  "paths": {
    "/users/{action}": {
      "name": "/users/{action}",
      "lambdaFunction": "usersLambda",
      "permissions": {
        "setting": "open"
      }
    },
    "/appointments/{action}": {
      "name": "/appointments/{action}",
      "lambdaFunction": "appointmentsLambda",
      "permissions": {
        "setting": "open"
      }
    },
    "/articles": {
      "name": "/articles",
      "lambdaFunction": "articlesLambda",
      "permissions": {
        "setting": "open"
      }
    }
  }
}